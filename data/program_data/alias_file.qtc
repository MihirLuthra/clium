D_OUTPUT_DIR =
	[BASH](
		dir="[VAR](OUTPUT_DIR)/$(dirname '[VAR](INSTANCE)'\)"
		mkdir -p "$dir" || return 1
		echo -n "$dir"
	)

FILE_TO_OPEN =
	[VAR](FILE_TO_OPEN)

INSTANCE =
	[VAR](INSTANCE)

INSTANCE_PATH =
	[VAR](INSTANCES_DIR)/[VAR](INSTANCE)

OUTPUT_DIR =
	[VAR](OUTPUT_DIR)

OUTPUT_PATH =
	[VAR](OUTPUT_DIR)/[VAR](INSTANCE).out
